{
  "name": "secret-card",
  "version": "1.0.0",
  "description": "mongodb, express, react, nodejs",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ervingg/secret-card.git"
  },
  "keywords": [
    "javascript",
    "mongodb",
    "express",
    "react",
    "nodejs",
    "mern"
  ],
  "author": "Khamoido Ervin <hamoydoerik@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ervingg/secret-card/issues"
  },
  "homepage": "https://github.com/ervingg/secret-card#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
